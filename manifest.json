{
  "name": "Quran Extension",
  "version": "1.0",
  "description": "View Quran verses on your homepage.",
  "permissions": [
          "storage"
  ],
  "chrome_url_overrides" : {
    "newtab": "newtab.html"
  },
  "manifest_version": 2,
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "icons": {
    "16": "assets/icon-16.png",
    "32": "assets/icon-32.png",
    "48": "assets/icon-48.png",
    "128": "assets/icon-128.png"
  }
}
